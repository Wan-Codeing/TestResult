package test1;

public class Sugar extends BreadType {
	public String RecipeSugar() {
		protected int sugar = 200;
		flour = 100;
		water = 50;
		
		String result = "breadType : sugar\n" + "receipe\n" + "flour : " + flour + "\n" + "water : " + water + "\n" + "sugar : " + sugar;
		return result;
	}
}
