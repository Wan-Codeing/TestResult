package test;

public class Sugar extends BreadType {
	public void RecipeSugar() {
		int sugar = 200;
		flour = 100;
		water = 50;
		
		System.out.println("breadType : cream");
		System.out.println("receipe");
		System.out.println("flour : "+flour);
		System.out.println("water : "+water);
		System.out.println("sugar : "+sugar);
	}
}
