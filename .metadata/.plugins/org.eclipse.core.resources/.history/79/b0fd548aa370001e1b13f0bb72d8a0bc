package test3;

public class test3 {
	public static int factorial(int num) {
        int sum = 1;
        for(int i = num; i>=1; i--) {
            sum *= i; 
            if(i == 1) {
                System.out.print(i + " = ");
            }else {
                System.out.print(i +" x ");
            }
            
        }
        return sum;
    }
    
    public static void main(String[] args) {
        System.out.println(factorial(5));
    }
}
